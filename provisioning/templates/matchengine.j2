[Unit]
Description=Match engine
After=network.target alertcenter.service mysql.service kafka.service

[Service]
Type=forking
SyslogIdentifier=matchengine
ExecStart={{root_dir}}/matchengine/matchengine.exe {{root_dir}}/matchengine/config.json

Restart=always
PrivateTmp=true
TimeoutStopSec=60s
TimeoutStartSec=10s
StartLimitInterval=120s
StartLimitBurst=5

[Install]
WantedBy=multi-user.target
