[Unit]
Description=Http router
After=network.target alertcenter.service matchengine.service marketprice.service readhistory.service

[Service]
Type=forking
SyslogIdentifier=accesshttp
ExecStart={{root_dir}}/accesshttp/accesshttp.exe {{root_dir}}/accesshttp/config.json

Restart=always
PrivateTmp=true
TimeoutStopSec=60s
TimeoutStartSec=10s
StartLimitInterval=120s
StartLimitBurst=5

[Install]
WantedBy=multi-user.target
